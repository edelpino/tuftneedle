I"y(function() {
  var addCartItems;

  $(function() {
    $("input:radio").change(function() {
      var dimensions, price;
      if ($(this).prop("checked", true)) {
        price = "<b>" + ($(this).data('price')) + "</b>";
        dimensions = "<b>" + ($(this).data('dimensions')) + "</b>";
        return $("#product_details").html(price + "<br/>" + dimensions);
      }
    });
    $("#add_to_cart").click(function(e) {
      var $form, $hidden_input, $input, $label, product_id;
      e.preventDefault();
      $input = $("input:radio:checked");
      $label = $("label[for='" + ($input.attr("id")) + "']");
      $form = $('#new_order');
      product_id = $input.val();
      $hidden_input = "<input type='hidden' name='" + ($input.attr('name')) + "' value='" + product_id + "' />";
      $form.prepend($hidden_input);
      addCartItems(product_id, $label);
      return $.each($form.find("input:hidden"), function(index, value) {
        return $(this).attr('name', $(this).attr('name').replace(/\d+/g, index));
      });
    });
    return $("[name='sales_channel_filter']").change(function(e) {
      var $key, $target_html;
      $key = $(e.target).find('option:selected');
      $target_html = $('orders_table');
      console.log(key);
      return $.ajax("/orders/ajax_filter_orders/" + ($(key).val()), {
        error: function(jqXHR, textStatus, errorThrown) {},
        success: function(data) {
          return $target_html.html(data);
        }
      });
    });
  });

  addCartItems = function(product_id, label) {
    var $duplicateLi, cost, count, productName;
    $duplicateLi = $("[productId='" + product_id + "'");
    cost = parseFloat(label.attr("cost"));
    productName = label.attr('productName');
    if ($duplicateLi.length > 0) {
      count = $("input:hidden[value='" + product_id + "']").length;
      cost = parseFloat(label.attr("cost")) * count;
      return $duplicateLi.text("(" + count + ") " + productName + " $" + (cost.toFixed(2)) + " ");
    } else {
      return $("#cart").append("<li productId='" + product_id + "' class='list-group-item'>" + productName + " " + (cost.toFixed(2)) + "</li>");
    }
  };

}).call(this);
